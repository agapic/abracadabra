                <div class="divider"></div>
                <li><a href="/">Homepage</a></li>
          </ul>
      </div>
  </div>
    <div class="col-md-8">
      <h3>Region: {{region}} Gamemode: {{gamemode}}</h3>
    </div>
  </div>
  <div id="row col-md-12 datatable">
      <table class = "ui striped table">
          <thead>
              <tr>
                  <th>Champion Name</th>
                  <th>Patch 5.11 Magic Damage</th>
                  <th>Patch 5.14 Magic Damage</th>
                  <th>Magic Damage Difference</th>
                  <th>Patch 5.11 - Damage Per Game</th>
                  <th>Patch 5.14 - Damage Per Game</th>
                  <th>Patch 5.11 Winrate</th>
                  <th>Patch 5.14 Winrate</th>
                  <th>Winrate Difference</th>
              </tr>
          </thead>

          <tbody>
              {{#each champData}}
              <tr>
                  <td> <b>{{champname}}</b></td>
                  <td> {{sum}}</td>
                  <td>{{afterSum}}</td>
                  <td class="{{damageColor}}">{{damageDifference}}%</td>
                  <td>{{damagePerGameBefore}} ({{gamesBefore}} game(s))</td>
                  <td>{{damagePerGameAfter}} ({{gamesAfter}} game(s))</td>
                  <td> {{gamesBeforeWinrate}}</td>
                  <td>{{gamesAfterWinrate}}</td>
                  <td class="{{winColor}}"> {{winrateDifference}}%</td>
              </tr>

              {{/each}}
          </tbody>
      </table>
  </div>
</div>


