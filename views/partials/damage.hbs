                <li class="dropdown-submenu">
                  <a tabindex="-1" href="#">Ranked</a>
                  <ul class="dropdown-menu">
                      <li><a tabindex="-1"  href="/champions/RANKED/BR">Brazil</a></li>
                      <li><a tabindex="-1"  href="/champions/RANKED/EUNE">Europe Northeast</a></li>
                      <li><a tabindex="-1"  href="/champions/RANKED/EUW">Europe West</a></li>
                      <li><a tabindex="-1"  href="/champions/RANKED/KR">Korea</a></li>
                      <li><a tabindex="-1"  href="/champions/RANKED/LAN">Latin America North</a></li>
                      <li><a tabindex="-1"  href="/champions/RANKED/LAS">Latin America South</a></li>
                      <li><a tabindex="-1"  href="/champions/RANKED/NA">North America</a></li>
