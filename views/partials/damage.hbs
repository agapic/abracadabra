<div id="datatable">
<table class = "ui striped table">
    <thead><tr><th>Champion Name</th><th>Magic Damage Before</th><th>Magic Damage After</th><th>Difference</th></tr></thead>
    <tbody>

        {{#each champDamageData}}
        
        <tr>
            <td> {{champname}}</td> 
            <td> {{sum}}</td>
            <td>{{afterSum}}</td>
            <td>{{difference}}%</td>
        </tr>
        
        {{/each}} 

    </tbody></table>
        
        

    </div>

<script>
$('.ui.dropdown')
    .dropdown();

        if(window.location.pathname == "/damage/"){
       $("#datatable").load("/damage #datatable table")
        }
</script>


     